{
    "author": "Sebastian Blask",
    "devDependencies": {
        "eslint": "3.13.x",
        "jscs": "2.6.x",
        "web-ext": "1.9.x"
    },
    "license": "MIT",
    "name": "opentabsnexttocurrent",
    "repository": {
        "type": "git",
        "url": "https://github.com/sblask/firefox-open-tabs-next-to-current"
    },
    "scripts": {
        "build": "web-ext build --artifacts-dir . --ignore-files screenshots --ignore-files package.json --ignore-files icon.fodg --ignore-files *.png",
        "help": "web-ext --help",
        "lint": "eslint --debug *.js && jscs *.js",
        "start": "GTK_THEME=Greybird web-ext run --verbose --firefox firefox-trunk",
        "start:stable": "GTK_THEME=Greybird web-ext run --verbose --firefox firefox",
        "test": "web-ext lint"
    },
    "title": "Open Tabs Next to Current",
    "version": "2.0.5"
}
